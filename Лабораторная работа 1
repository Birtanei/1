print('ax^2+bx+c=0 - квадратное уравнение')
a=float(input('Введите коэффициент a:'))
b=float(input('Введите коэффициент b:'))
c=float(input('Введите коэффициент c:'))
D=b*b - 4*a*c
print()
if D<0:
    print('Корней нет')
elif D==0:
    print('Корень уравнения равен',-b/(2*a))
else:
    print('Корни уравнения равны:')
    print('x1=',(-b-D**0.5)/(2*a))
    print('x2=',(-b+D**0.5)/(2*a))
